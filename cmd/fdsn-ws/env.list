DB_HOST=localhost
DB_USER=fdsn_w
DB_PASSWD=test
DB_NAME=fdsn
DB_SSLMODE=disable
DB_CONN_TIMEOUT=5
DB_MAX_IDLE_CONNS=20
DB_MAX_OPEN_CONNS=30

# Shared key for https basic auth.  Should be sent in the password field.
FDSN_KEY=test

# The station service is proxied.
FDSN_STATION=service.geonet.org.nz

# The directory to use for large tempfiles (automatically deleted after use).
# Leave empty to use system default temp dir.
SCRATCH_DIR=

# The AWS S3 bucket that holds the miniseed files served by dataselect.
# AWS_ACCESS_KEY_ID and AWS_SECRET_ACCESS_KEY must be properly set to
# access this bucket.
AWS_REGION=ap-southeast-2
S3_BUCKET=fdsn-travis-test.geonet.org.nz
